{
  "name": "embedding-batch-workload-template",
  "version": "1.0.0",
  "description": "FSx for NetApp ONTAP Embedding Batch Workload Template",
  "bin": {
    "embedding-workload": "bin/embedding-workload-app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "ts-node test/test-runner.ts",
    "test:unit": "ts-node test/test-runner.ts --unit",
    "test:integration": "ts-node test/test-runner.ts --integration",
    "test:e2e": "ts-node test/test-runner.ts --e2e",
    "test:performance": "ts-node test/test-runner.ts --performance",
    "test:all": "ts-node test/test-runner.ts --all",
    "test:coverage": "ts-node test/test-runner.ts --unit --coverage",
    "test:watch": "ts-node test/test-runner.ts --unit --watch",
    "e2e": "ts-node test/e2e/e2e-test-runner.ts",
    "e2e:minimal": "ts-node test/e2e/e2e-test-runner.ts minimal",
    "e2e:production": "ts-node test/e2e/e2e-test-runner.ts production",
    "e2e:script": "../scripts/run-e2e-tests.sh",
    "e2e:script:minimal": "../scripts/run-e2e-tests.sh minimal",
    "e2e:script:production": "../scripts/run-e2e-tests.sh production",
    "e2e:dry-run": "../scripts/run-e2e-tests.sh --dry-run",
    "e2e:cleanup": "../scripts/run-e2e-tests.sh --cleanup-only",
    "validate:real-data": "../scripts/validate-real-data-workflow.sh",
    "validate:real-data:verbose": "../scripts/validate-real-data-workflow.sh --verbose",
    "analyze:embedding-quality": "python ../scripts/analyze-embedding-quality.py",
    "jest": "jest",
    "cdk": "cdk",
    "deploy": "cdk deploy --require-approval never",
    "destroy": "cdk destroy --force",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.3",
    "@types/uuid": "^9.0.7",
    "aws-cdk": "2.129.0",
    "cdk-nag": "^2.37.55",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk-lib": "2.129.0",
    "aws-sdk": "^2.1691.0",
    "constructs": "^10.0.0",
    "uuid": "^9.0.1"
  },
  "keywords": [
    "aws",
    "cdk",
    "fsx",
    "netapp",
    "ontap",
    "embedding",
    "batch",
    "rag",
    "bedrock",
    "ai",
    "machine-learning"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/embedding-batch-workload-template.git"
  },

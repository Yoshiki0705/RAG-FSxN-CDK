  "ScheduleExpression": "cron(0 2 * * ? *)",
  "State": "ENABLED",
  "Targets": [
    {
      "Id": "1",
      "Arn": "arn:aws:batch:ap-northeast-1:178625946981:job-queue/sqlite-embedding-job-queue",
      "RoleArn": "arn:aws:iam::178625946981:role/AWSBatchServiceRole",
      "BatchParameters": {
        "JobDefinition": "sqlite-embedding-batch-job-def",
        "JobName": "sqlite-embedding-scheduled"
      }
    }
  ]
}
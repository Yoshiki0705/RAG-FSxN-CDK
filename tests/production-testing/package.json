{
  "name": "production-testing",
  "version": "1.0.0",
  "description": "AWS東京リージョン本番環境でのChatbot UIテストシステム",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:production": "node dist/index.js",
    "test:connection": "node dist/scripts/test-connection.js",
    "test:auth": "node dist/scripts/test-authentication.js",
    "test:auth:comprehensive": "ts-node scripts/run-authentication-tests.ts",
    "test:auth:sid": "INCLUDE_BASIC_AUTH=false INCLUDE_MULTI_REGION=false ts-node scripts/run-authentication-tests.ts",
    "test:auth:multi-region": "INCLUDE_BASIC_AUTH=false INCLUDE_SID_AUTH=false ts-node scripts/run-authentication-tests.ts",
    "test:auth:basic": "INCLUDE_SID_AUTH=false INCLUDE_MULTI_REGION=false ts-node scripts/run-authentication-tests.ts",
    "test:ai:comprehensive": "ts-node scripts/run-ai-integration-tests.ts",
    "test:ai:nova": "INCLUDE_JAPANESE_ACCURACY=false INCLUDE_STREAMING=false INCLUDE_MULTIMODAL=false ts-node scripts/run-ai-integration-tests.ts",
    "test:ai:japanese": "INCLUDE_NOVA_MODELS=false INCLUDE_STREAMING=false INCLUDE_MULTIMODAL=false ts-node scripts/run-ai-integration-tests.ts",
    "test:ai:streaming": "INCLUDE_NOVA_MODELS=false INCLUDE_JAPANESE_ACCURACY=false INCLUDE_MULTIMODAL=false ts-node scripts/run-ai-integration-tests.ts",
    "test:ai:multimodal": "INCLUDE_NOVA_MODELS=false INCLUDE_JAPANESE_ACCURACY=false INCLUDE_STREAMING=false ts-node scripts/run-ai-integration-tests.ts",
    "test:rag:comprehensive": "ts-node scripts/run-rag-integration-tests.ts",
    "test:rag:vector-search": "INCLUDE_SEARCH_INTEGRATION=false INCLUDE_CONTEXT_PERSISTENCE=false INCLUDE_PERMISSION_FILTERING=false ts-node scripts/run-rag-integration-tests.ts",
    "test:rag:search-integration": "INCLUDE_VECTOR_SEARCH=false INCLUDE_CONTEXT_PERSISTENCE=false INCLUDE_PERMISSION_FILTERING=false ts-node scripts/run-rag-integration-tests.ts",
    "test:rag:context-persistence": "INCLUDE_VECTOR_SEARCH=false INCLUDE_SEARCH_INTEGRATION=false INCLUDE_PERMISSION_FILTERING=false ts-node scripts/run-rag-integration-tests.ts",
    "test:rag:permission-filtering": "INCLUDE_VECTOR_SEARCH=false INCLUDE_SEARCH_INTEGRATION=false INCLUDE_CONTEXT_PERSISTENCE=false ts-node scripts/run-rag-integration-tests.ts",
    "test:ui:comprehensive": "ts-node scripts/run-ui-integration-tests.ts",
    "test:ui:responsive-design": "INCLUDE_REALTIME_CHAT=false INCLUDE_DOCUMENT_SOURCE_DISPLAY=false INCLUDE_ACCESSIBILITY=false ts-node scripts/run-ui-integration-tests.ts",
    "test:ui:realtime-chat": "INCLUDE_RESPONSIVE_DESIGN=false INCLUDE_DOCUMENT_SOURCE_DISPLAY=false INCLUDE_ACCESSIBILITY=false ts-node scripts/run-ui-integration-tests.ts",
    "test:ui:document-source-display": "INCLUDE_RESPONSIVE_DESIGN=false INCLUDE_REALTIME_CHAT=false INCLUDE_ACCESSIBILITY=false ts-node scripts/run-ui-integration-tests.ts",
    "test:ui:accessibility": "INCLUDE_RESPONSIVE_DESIGN=false INCLUDE_REALTIME_CHAT=false INCLUDE_DOCUMENT_SOURCE_DISPLAY=false ts-node scripts/run-ui-integration-tests.ts",
    "test:performance:comprehensive": "ts-node scripts/run-performance-integration-tests.ts",
    "test:performance:response-time": "INCLUDE_CONCURRENT_LOAD=false INCLUDE_UPTIME_MONITORING=false INCLUDE_MULTI_REGION_SCALABILITY=false ts-node scripts/run-performance-integration-tests.ts",
    "test:performance:concurrent-load": "INCLUDE_RESPONSE_TIME=false INCLUDE_UPTIME_MONITORING=false INCLUDE_MULTI_REGION_SCALABILITY=false ts-node scripts/run-performance-integration-tests.ts",
    "test:performance:uptime-monitoring": "INCLUDE_RESPONSE_TIME=false INCLUDE_CONCURRENT_LOAD=false INCLUDE_MULTI_REGION_SCALABILITY=false ts-node scripts/run-performance-integration-tests.ts",
    "test:performance:multi-region-scalability": "INCLUDE_RESPONSE_TIME=false INCLUDE_CONCURRENT_LOAD=false INCLUDE_UPTIME_MONITORING=false ts-node scripts/run-performance-integration-tests.ts",
    "test:ai": "node dist/scripts/test-ai-response.js",
    "test:performance": "node dist/scripts/test-performance.js",
    "test:ui": "node dist/scripts/test-ui-ux.js",
    "test:security": "node dist/scripts/test-security.js",
    "test:integration": "node dist/scripts/test-integration.js",
    "test:all": "node dist/scripts/run-all-tests.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "dev": "ts-node src/index.ts"
  },
  "keywords": [
    "aws",
    "production-testing",
    "chatbot",
    "ui-testing",
    "bedrock",
    "cognito",
    "dynamodb",
    "opensearch",
    "fsx"
  ],
  "author": "NetApp Japan Technology Team",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.490.0",
    "@aws-sdk/client-cloudfront": "^3.490.0",
    "@aws-sdk/client-cloudwatch": "^3.490.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.490.0",
    "@aws-sdk/client-dynamodb": "^3.490.0",
    "@aws-sdk/client-fsx": "^3.490.0",
    "@aws-sdk/client-opensearchserverless": "^3.490.0",
    "@aws-sdk/client-wafv2": "^3.490.0",
    "@aws-sdk/client-xray": "^3.490.0",
    "dotenv": "^16.3.1",
    "playwright": "^1.40.0",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}